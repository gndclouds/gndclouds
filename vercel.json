{
  "version": 2,
  "buildCommand": "./vercel-install.sh && npm run build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "framework": "nextjs",
  "regions": ["iad1"],
  "redirects": [
    {
      "source": "/db/:path*",
      "destination": "https://raw.githubusercontent.com/gndclouds/db/main/:path*",
      "permanent": false
    }
  ]
}
